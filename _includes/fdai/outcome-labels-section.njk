<!-- Outcome Labels Section -->
<section style="background: #00FFD1; padding: 80px 20px; border-top: 8px solid #000;">
  <div style="max-width: 900px; margin: 0 auto; text-align: center;">
    <h2 style="font-size: 56px; font-weight: 900; margin: 0 0 30px 0; color: #000;">{{ fdaiSections.outcomeLabels.title }}</h2>
    <p style="font-size: 22px; font-weight: 600; color: #000; line-height: 1.6; margin: 0 0 40px 0;">{{ fdaiSections.outcomeLabels.description }}</p>
    
    {% if fdaiSections.outcomeLabels.image %}
    <img src="{{ fdaiSections.outcomeLabels.image }}" alt="Outcome Labels" style="max-width: 600px; width: 100%; border: 5px solid #000; box-shadow: 12px 12px 0 #000;" />
    {% endif %}

    {% if fdaiSections.outcomeLabels.link %}
    <a href="{{ fdaiSections.outcomeLabels.link.url }}" target="_blank" style="display: inline-block; background: #000; color: #fff; padding: 16px 32px; border: 5px solid #000; box-shadow: 8px 8px 0 #FF6B6B; font-weight: 900; font-size: 20px; text-decoration: none; margin-top: 40px;">
      {{ fdaiSections.outcomeLabels.link.text }}
    </a>
    {% endif %}
  </div>
</section>

